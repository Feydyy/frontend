//var numbers = [33, 11, -5, 20, 1, 55, 100, 2, 94, 55, 11]


var numbers = [33, 11, -5, 20, 1, 55, 100, 2, 94, 55, 11]
let maxNum = numbers[0];

for (let i = 1; i< numbers.length ; i++){
    if(numbers[i] > maxNum){
        maxNum = numbers[i];
    }
}

console.log(maxNum);

let minNum = numbers[0];

for (let i = 1; i < numbers.length ; i++){
    if(numbers[i] < minNum){
        minNum = numbers[i];
    }
}

console.log(minNum);

